<UserControl
    x:Class="LinkerPlayer.UserControls.PlaylistTabs"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:userControls="clr-namespace:LinkerPlayer.UserControls"
    d:DesignHeight="80"
    d:DesignWidth="800"
    mc:Ignorable="d">
    <Grid>

        <StackPanel Orientation="Horizontal">
            <TabControl Margin="0,20,0,0">
                <TabItem Name="Tab1" Header="Hello" />
                <TabItem Name="Tab3" Header="Hello World" />
                <TabItem Name="Tab2" Header="Hello World Again" />
            </TabControl>
            <userControls:NewPlaylistButton
                Grid.Row="0"
                Grid.Column="0"
                Margin="20,0,0,0" />
        </StackPanel>


    </Grid>
</UserControl>